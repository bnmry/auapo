<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
	"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<%= stylesheet_link_tag "screen", :media => "all" %>
<%= javascript_include_tag :defaults %>
<title>Alpha Phi Omega :: Eta Phi Chapter</title>
	
</head>


<body>

<div id="airlock">
	<img border="0" src="/images/top.gif"/>
	<table width="100%"  border="0" cellspacing="0" cellpadding="0" id="path">
		<tr>
			<td width="81%">
				<%= render(:partial => "/layouts/std_titlebar")%>
			</td>
		</tr>
	</table>

<!--Left-bar Navigation-->

<ul class="menu">
	<!-- Render navigation bar partials for specific user types. -->
	<%= render(:partial => "/layouts/std_menu")%>
	<%= render(:partial => "/layouts/std_menu_guests")%>
	<%= render(:partial => "/layouts/std_menu_brother")%>
	<%= render(:partial => "/layouts/std_menu_alumni")%>
</ul>

<!-- BEGIN Right-bar Navigation -->
<ul id="sidebar">
	<%= render(:partial => "/layouts/std_info_brothers")%>
	<%= render(:partial => "/layouts/std_info_guests")%>
	
</ul>
<!-- END Right-bar Navigation-->

<div id="content">
	<% for name in [:notice, :warning, :message] %>
	    <% if flash[name] %>
	      <%= "<div id=\"#{name}\">#{flash[name]}</div>" %>
	    <% end %>
	  <% end %>
<%= @content_for_layout %>
  </div>
	
	<%= render(:partial => "/layouts/footer")%>
			  
</div>
<script src="http://www.google-analytics.com/urchin.js" type="text/javascript">
</script>
<script type="text/javascript">
_uacct = "UA-504797-3";
urchinTracker();
</script>
</body>
</html>
