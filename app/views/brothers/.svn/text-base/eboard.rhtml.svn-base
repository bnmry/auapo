<h4>The Eta Phi Executive Board</h4>
<% for brother in @brothers%>
<% @user = brother%>
<% if !brother.picture.nil?%>
	<img style="margin:10px" width="100" src="<%= url_for_file_column 'user', 'picture', 'medium' %>">
	<span style="font-size:large"><%= brother.firstname%> <%= brother.lastname%></span>
	<br>
<% end %>
<% end %>